<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <welcome-file-list>
    <welcome-file>index2.jsp</welcome-file>
  </welcome-file-list>
  <filter>
    <filter-name>EncodingFilter</filter-name>
    <filter-class>filter.EncodingFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>EncodingFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <servlet>
    <servlet-name>UserServlet</servlet-name>
    <servlet-class>servlet.UserServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ArticleServlet</servlet-name>
    <servlet-class>servlet.ArticleServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ArticleTypeServlet</servlet-name>
    <servlet-class>servlet.ArticleTypeServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>WareServlet</servlet-name>
    <servlet-class>servlet.WareServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>VideosServlet</servlet-name>
    <servlet-class>servlet.VideosServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>TeaParentServlet</servlet-name>
    <servlet-class>servlet.TeaParentServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>TeaChildServlet</servlet-name>
    <servlet-class>servlet.TeaChildServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>TeaArticleServlet</servlet-name>
    <servlet-class>servlet.TeaArticleServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LiuYanServlet</servlet-name>
    <servlet-class>servlet.LiuYanServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>IndexServlet</servlet-name>
    <servlet-class>servlet.IndexServlet</servlet-class>
  </servlet>




  <servlet-mapping>
    <servlet-name>UserServlet</servlet-name>
    <url-pattern>/UserServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ArticleServlet</servlet-name>
    <url-pattern>/ArticleServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ArticleTypeServlet</servlet-name>
    <url-pattern>/ArticleTypeServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>WareServlet</servlet-name>
    <url-pattern>/WareServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>VideosServlet</servlet-name>
    <url-pattern>/VideosServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>TeaParentServlet</servlet-name>
    <url-pattern>/TeaParentServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>TeaChildServlet</servlet-name>
    <url-pattern>/TeaChildServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>TeaArticleServlet</servlet-name>
    <url-pattern>/TeaArticleServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LiuYanServlet</servlet-name>
    <url-pattern>/LiuYanServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>IndexServlet</servlet-name>
    <url-pattern>/IndexServlet</url-pattern>
  </servlet-mapping>
  
</web-app>